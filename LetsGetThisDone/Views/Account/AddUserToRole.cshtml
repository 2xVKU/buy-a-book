@model LetsGetThisDone.Models.AddToRoleModel

@{
    ViewBag.Title = "AddUserToRole";
}

<h2>AddUserToRole</h2>

@using (Html.BeginForm("AddUserToRole", "Account"))
{
    <div class="form-group">
        @Html.LabelFor(e => e.Email)
        @Html.TextBoxFor(e => e.Email)
    </div>
    <div class="form-group">
        @Html.DropDownListFor(e => e.Role, new SelectList(Model.roles), new { @class = "form-control" })
    </div>
    <button type="submit">Save</button>
}


